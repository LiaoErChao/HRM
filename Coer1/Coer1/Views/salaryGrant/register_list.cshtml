
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <script src="~/jquery/dist/jquery.js"></script>
    <script src="~/vue.js"></script>
    <script src="~/comm.js"></script>
    <link href="~/css/table.css" rel="stylesheet" />
    <title>无标题文档</title>


    <STYLE type="text/css">
        td {
            text-align: center
        }
    </STYLE>
</head>

<body>
    <div id="div1">
        <form method="post" asp-action="">

            <table width="100%">
                <tr>
                    <td colspan="2" style="text-align: left">
                        <font color="black">
                            您正在做的业务是：人力资源--薪酬标准管理--薪酬发放登记(负责人控制)
                        </font>

                    </td>
                </tr>

            </table>
            <br>
            薪酬总数:{{count}}，
            总人数:{{rows}}，基本薪酬总数:{{salary_sum}}.0，实发总额:{{paid_sum}}
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">

                <tr>
                    <td width="10%" class="TD_STYLE1">
                        <span>序号</span>
                    </td>


                    <td width="40%" class="TD_STYLE1">
                        <span>{{jg}}</span>
                    </td>

                    <td width="10%" class="TD_STYLE1">
                        <span>人数</span>
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        基本薪酬总额(元)
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        登记
                    </td>
                </tr>


                <tr class="TD_STYLE2" v-for="(item,index) in da">
                    <td>
                        {{item.fstid}}
                    </td>

                    <td>
                        {{item.fstname}}
                    </td>
                    <td>
                        {{item.count}}
                    </td>
                    <td>
                        {{item.salasum}}
                    </td>
                    <td>
                        <a v-bind:href="'/salaryGrant/register_commit/?fstname='+[item.fstname]">登 记</a>
                    </td>
                </tr>
            </table>
            <p>

                &nbsp;
            </p>
        </form>
    </div>
 <script>
    
     var vm = new Vue({
         el: "#div1",
         data: {
             da: [],
             jg: "",
             salary_sum: "",
             paid_sum: "",
             rows: "",
             rs:"",
             count:"",

         },
         methods: {
             load: function () {
               
                 var t = this;
                 $.ajax({
                     url: "/salaryGrant/register_list1",
                     type: "post",                
                     success: function (msg) {
                         var z = eval("(" + msg + ")");
                         t.da = z["dt"];   
                         t.jg = z["jg"];
                         t.salary_sum = z["salary_sum"];
                         t.paid_sum = z["paid_sum"];
                         t.rows = z["rows"];
                        
                         t.count = z["index"];
                     }

                 });

             },
            
         },
         mounted: function () {
             this.load();
            
         }

     });

 </script>

</body>
</html>
